{%- endif -%}

{%- case yoast_seo_page_type -%}
	{%- when "page" -%}{{- page.metafields.yoast_seo.indexable[variable] -}}
	{%- when "article" -%}{{- article.metafields.yoast_seo.indexable[variable] -}}
	{%- when "product" -%}{{- product.metafields.yoast_seo.indexable[variable] -}}
	{%- when "collection" -%}{{- collection.metafields.yoast_seo.indexable[variable] -}}
	{%- when "blog" -%}{{- blog.metafields.yoast_seo.indexable[variable] -}}
{%- endcase -%}
