        <style></style>

{% assign nm_easywholesale_the_page = false %}   
{% assign nm_easywholesale_goToCart = true %}   
{% assign nm_wholesale_qtyStep_product = 1 %}
{% assign nm_easywholesale_showVariantImage = true %}        
{% assign nm_easywholesale_showPrices = true %}
{% assign nm_easywholesale_addHtml = "" %}
{% assign nm_easywholesale_textOutOfStock = "out of stock" %}
{% assign nm_easywholesale_deleteTextOutOfStock = "" %}
{% assign nm_easywholesale_add_to_cart = "Добавить в корзину" %}  
{% assign nm_easywholesale_showAllOutOfStock = false %}
{% assign nm_easywholesale_showForm = true %} 

 

{% capture the_snippet_content %}{% include "wholesale-product-config" %}{% endcapture %}
{% unless the_snippet_content contains "Liquid error" %}
  {% include "wholesale-product-config" %}
{% endunless %}

{% unless nm_easywholesale_goToCart %}
<script>
    var nm_easywholesale_goToCart = 1;
</script>
{% endunless %}

{% if nm_easywholesale_showForm %}

<div id="nm-display-whole" style="display: none;">    
  
<div class="nm-wholesale-main nm-easywholesale-container" style="display: none;">        
<input type="hidden" id="nm-wholesale-page-type" value="product">
<input type="hidden" id="nm_wholesale_qtyStep_product" value="{{ nm_wholesale_qtyStep_product }}">
<form method="post" action="/cart/" id="product_form_product" accept-charset="UTF-8" enctype="multipart/form-data" class="nm-easywholesale-form">

   
<div data-title="{{ product.title }}" data-sku="" data-collection="" data-productid="{{ product.id }}" id="nm-wholesale-table-container-{{ product.id }}" class="nm-wholesale-table-container" style="display:none;">


              <div class="nm-easywholesale-header nm-secondary-border-color">
                        {% assign count_options = product.options_with_values.size %}
                        {% assign t_count = 0 %}
                            {% for product_option in product.options_with_values %}
                                {% assign t_count = t_count | plus: 1 %}
    
                                {% if product_option.values.size > 1 %}    
                    <div class="nm-easywholesale-select-container">
                        <select data-id="{{ product.id }}" class="nm-wholesale-filter-{{ product.id }} nm-wholesale-filter nm-primary-color" data-option="{{ product_option.name }}"
                                  
                                  {% if t_count != count_options %}
                                    style="padding-right: 15px !important"
                                  {% else %}
                                    style="padding-right: 15px !important"
                                  {% endif %}
                                  
                                  >
    
                            
                          	{% assign all_var = product_option.name %}
                          
                          	{% if translateOptionKeys and translateOptionValues %}
                          		{% assign count = 0 %}
                          		{% for translateOption in translateOptionKeys %}
                          			{% if product_option.name == translateOption %}
                          				{% assign all_var = translateOptionValues[count]%}
                          			{% endif %}
                          			{% assign count = count | plus:1 %}
                          		{% endfor %}
                          	{% endif %}
                          
                            <option data-text="All" value="all_variants">{{ all_var }}</option>
    
                            {% for value in product_option.values %}
                            <option value="{{ value }}">{{ value }}</option>
                            {% endfor %}
                        </select>
                        <span class="nm-svg"
                        {% if t_count == count_options %}
                                  style="position: relative; right: 7px;"
                            {% endif %}
                        >
                            <svg class="nm-primary-color-fill svg-angle-down" role="img" viewBox="0 0 256 512" width="8" style="fill: rgb(52, 61, 71);"><path d="M119.5 326.9L3.5 209.1c-4.7-4.7-4.7-12.3 0-17l7.1-7.1c4.7-4.7 12.3-4.7 17 0L128 287.3l100.4-102.2c4.7-4.7 12.3-4.7 17 0l7.1 7.1c4.7 4.7 4.7 12.3 0 17L136.5 327c-4.7 4.6-12.3 4.6-17-.1z"></path></svg>
                        </span>
                    </div>
                            {% endif %}
                            {% endfor %}
  
                
            </div>


                            {% assign count_variant = 0 %}
                            {% assign set_foto = 0 %}
                            
                            {% for variant in product.variants %}
                                {% unless variant.inventory_policy == "deny" and variant.inventory_quantity == 0 and variant.inventory_management == "shopify" %}
                                  	{% if variant.available %}
                            			{% if variant.image and nm_easywholesale_showVariantImage %}
                            				{% assign set_foto = 1 %}
                            		    {% endif %} 
                                        {% assign count_variant = count_variant | plus: 1 %}
                                  	{% endif %}
                                {% endunless %}
                            {% endfor %}
                            
                            {% assign easywholesale_show_add = true %}
                            {% if count_variant < 2 %}
                               {% assign easywholesale_show_add = false %} 
                            {% endif %}

          <div class="nm-easywholesale-table nm-secondary-border-color nm-easywholesale-table-product" id="nm-wholesale-table-body-{{ product.id }}">

            <div id="nm-no-product-matched-{{ product.id }}" class="nm-no-product-matched">
                <p>No products matched your search</p>
            </div>  
            


              {% for variant in product.variants %}
                {% unless variant.inventory_policy == "deny" and variant.inventory_quantity == 0 and variant.inventory_management == "shopify" %}
                  	{% if variant.available %}
                    
                        {% assign variant_title = variant.title %}
                        
                        {% assign inventory_police_temp = variant.inventory_policy %}
                        
                                        {% unless variant.inventory_management %}
                                            {% assign inventory_police_temp = "continue" %}
                                        {% endunless %}
                                        
                        {% if variant_title == "Default Title" %}{% assign variant_title = "" %}{% endif %}   
                           
                  <div class="nm-easywholesale-item nm-layout-border-color nm-wholesale-table-item-{{ product.id }}" data-option1="{{ variant.option1 }}" data-option2="{{ variant.option2 }}" data-option3="{{ variant.option3 }}">
                  
                      <div class="nm-easywholesale-name-and-price" id="nm-easywholesale-name-and-price-{{ variant.id }}">  
                      
                                        {% if variant.image and nm_easywholesale_showVariantImage  %}
                            				<div class="nm-easywholesale-img"><img src="{{ variant.image | img_url: '40x40' }}"></div> 
                            		    {% else %} 
                                          {% if set_foto == 1%}
                                              <div class="nm-easywholesale-img"></div> 
                                          {% endif %}
                                        {% endif %} 

{% assign variant_price = variant.price %}           
{% capture the_snippet_content %}{% include "wholesale-page-callback-price" %}{% endcapture %}

                        <div class="nm-easywholesale-name-and-price-only">
                            <div class="nm-easywholesale-name nm-primary-color">{{ variant_title }}{% if variant.inventory_quantity > 0 or nm_easywholesale_showAllOutOfStock %} <small>(in stock: {{ variant.inventory_quantity }})</small>{% endif %}</div>
                            {% if nm_easywholesale_showPrices %}<div class="nm-easywholesale-price nm-primary-color" data-priceid="{{ variant.id }}">{{ variant_price | money }}</div>{% endif %}
                        </div>
                      </div>
                      {% if nm_easywholesale_showPrices %}
                      <div class="nm-easywholesale-qty-container">
                        <div class="nm-easywholesale-qty-decrease nm-layout-text-color" data-id="nm-{{ variant.id }}" id="nm-id-decrease-nm-{{ variant.id }}">&minus;</div>
                        <input type="text" class="nm-easywholesale-qty nm-layout-text-color nm-layout-border-color" data-id="nm-{{ variant.id }}" data-price="{{ variant_price }}" value="0" data-policy="{{ inventory_police_temp }}" data-quantity="{{ variant.inventory_quantity }}" id="nm-{{ variant.id }}" name="updates[{{ variant.id }}]"/>
                        <div class="nm-easywholesale-qty-increase nm-layout-text-color" data-id="nm-{{ variant.id }}" id="nm-id-increase-nm-{{ variant.id }}">&plus;</div>
                      </div>
                      {% endif %}
                  
                  
                  </div>
                  	{% endif %}
                {% endunless %}
              {% endfor %}


          </div>


  
            <div class="nm-secondary-border-color nm-wholesale-table-under-footer" id="nm-wholesale-table-under-footer-{{ product.id }}">
                <svg class="nm-secondary-color-fill svg-angle-down svg-angle-down_show_more" xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24"><path d="M6 0l12 12-12 12z"/></svg>
                <button data-productid="{{ product.id }}" id="nm-showMoreVariants-{{ product.id }}" class="nm-wholesale-show-more-variants nm-secondary-text-color nm-showMoreVariants-{{ product.id }}" type="button">
                    Show more <span id="nm-show-more-how-much"></span> variants <span id="nm-show-from-much-else"></span>
                </button>
            </div>
 
              <div class="nm-easywholesale-footer">
    
                <div class="nm-easywholesale-selected nm-layout-text-color">
                  Всего выбранных товаров: <span id="nm-total-selected-items" class="nm-total-selected-items">0</span>
                </div>
                <div class="nm-easywholesale-total nm-layout-text-color" id="nm-total-summ-selected">{{ 0 | money }}</div>
                
              </div>

          <div class="nm-easywholesale-add-to-cart-container">
          
            <button type="button" class="nm-wholesale-btn-add-to-cart nm-secondary-background-color" data-formname="product">
              {{ nm_easywholesale_add_to_cart }}
            </button>
            
          </div>
          

      
    </div>
{{ nm_easywholesale_addHtml }}    
</form>
</div>
</div>

{% endif %}
        <script>
            var layoutValues = {
                "LayoutMoreVariantsText" : "показать больше вариантов",
                "VariantsPerPage" : 6,
                "HeightRowsLimit" : 10,
                "addToCartSlicky" : 1,
                "currency"  : '{{ 0 | money | replace: "0.00", "X.XX" | replace: "0,00", "X,XX" }}',
                "money_format" : '{% raw %}₴{{amount}} UAH{% endraw %}'
            };
        </script>{% endif %}{% unless easywholesaleShow %}<script>
if (document.getElementById("nm-display-none-form")) document.getElementById("nm-display-none-form").style.display="";</script>{% endunless %}{% if product.id %}{% if easywholesale_show_add %}<script>var nm_easywholesale_show = true;</script>{% else %}<script>var nm_easywholesale_show = false;</script>{% endif %}{% endif %}